<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.chenhs.MySpringBoot.sqliteDemo.Mapper.RMapper">
  <select id="selectAll" resultType="com.chenhs.MySpringBoot.sqliteDemo.Entity.R">  
       select id,name from zz 
  </select>
  <insert id="insert">
    insert into zz values (#{id},#{name})
  </insert>
</mapper> 